enum Permission {
  ADMIN
  USER
  ITEMDELETE
  ITEMUPDATE
  ITEMCREATE
  PERMISSIONSUPDATE
}

type User {
  id: ID! @id
  name: String!
  email: String!
  password: String!
  permissions: [Permission] @scalarList(strategy: RELATION)
  avatar: String!
}

type SuccessMessage {
   id: ID! @id
   message : String!
}
