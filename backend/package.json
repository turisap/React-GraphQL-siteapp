    "bcryptjs": "2.4.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "6.0.0",
    "graphql": "^0.13.2",
    "graphql-cli": "^2.16.7",
    "graphql-yoga": "1.16.2",
    "jsonwebtoken": "8.3.0",
    "mjml": "^4.4.0-beta.1",
    "nodemailer": "^6.4.16",
    "nodemon": "1.18.4",
    "npm": "^6.4.1",
    "npm-run-all": "^4.1.3",
    "prisma": "1.17.1",
    "prisma-binding": "2.1.6",
    "randomstring": "^1.1.5",
    "stripe": "^6.12.1"
